<h1>Users#index</h1>
<%= link_to "View", users_path, :class => "link" %>
<br><br>
<% if user_signed_in? %>
    <P>Logged in as <strong><%= current_user.email %></strong></P>
    <% if can? :account_update, @user %>
      <%= link_to 'Edit profile', edit_user_registration_path, :class => "link" %> 
    <% end %> <br><br>
    <%= link_to "Logout", destroy_user_session_path, data: { turbo_method: :delete }, :class => "link" %>
<% else %>
  <%= link_to "Sign up", new_user_registration_path, :class => "link" %> <br><br>
  <%= link_to "Login", new_user_session_path, :class => "link" %>
<% end %>
